{
  "id": "complex-conditionals-v1",
  "version": "1.0.0",
  "metadata": {
    "title": "Complex Conditional Logic Example",
    "createdAt": "2025-10-27T17:29:31.846Z",
    "updatedAt": "2025-10-27T17:29:31.846Z",
    "description": "Demonstrates multi-level conditional dependencies"
  },
  "questions": [
    {
      "id": "level1_question",
      "type": "boolean",
      "text": "Do you own a car?",
      "required": true
    },
    {
      "id": "level2_car_type",
      "type": "single_choice",
      "text": "What type of car do you own?",
      "required": false,
      "options": [
        {
          "value": "electric",
          "label": "Electric"
        },
        {
          "value": "hybrid",
          "label": "Hybrid"
        },
        {
          "value": "gasoline",
          "label": "Gasoline"
        },
        {
          "value": "diesel",
          "label": "Diesel"
        }
      ],
      "conditional": {
        "dependsOn": "level1_question",
        "operator": "equals",
        "value": true,
        "action": "show"
      }
    },
    {
      "id": "level3_electric_brand",
      "type": "single_choice",
      "text": "Which electric car brand?",
      "required": false,
      "options": [
        {
          "value": "tesla",
          "label": "Tesla"
        },
        {
          "value": "rivian",
          "label": "Rivian"
        },
        {
          "value": "lucid",
          "label": "Lucid"
        },
        {
          "value": "other",
          "label": "Other"
        }
      ],
      "conditional": {
        "dependsOn": "level2_car_type",
        "operator": "equals",
        "value": "Electric",
        "action": "show"
      }
    },
    {
      "id": "level4_tesla_model",
      "type": "single_choice",
      "text": "Which Tesla model?",
      "required": false,
      "options": [
        {
          "value": "model_s",
          "label": "Model S"
        },
        {
          "value": "model_3",
          "label": "Model 3"
        },
        {
          "value": "model_x",
          "label": "Model X"
        },
        {
          "value": "model_y",
          "label": "Model Y"
        },
        {
          "value": "cybertruck",
          "label": "Cybertruck"
        }
      ],
      "conditional": {
        "dependsOn": "level3_electric_brand",
        "operator": "equals",
        "value": "Tesla",
        "action": "show"
      }
    },
    {
      "id": "level5_satisfaction",
      "type": "rating",
      "text": "How satisfied are you with your Tesla?",
      "required": false,
      "validation": {
        "min": 1,
        "max": 5
      },
      "conditional": {
        "dependsOn": "level3_electric_brand",
        "operator": "equals",
        "value": "Tesla",
        "action": "show"
      }
    },
    {
      "id": "level3_hybrid_reason",
      "type": "text",
      "text": "Why did you choose a hybrid vehicle?",
      "required": false,
      "validation": {
        "maxLength": 300
      },
      "conditional": {
        "dependsOn": "level2_car_type",
        "operator": "equals",
        "value": "Hybrid",
        "action": "show"
      }
    },
    {
      "id": "alternative_transport",
      "type": "multiple_choice",
      "text": "What alternative transportation do you use?",
      "required": false,
      "options": [
        {
          "value": "public_transit",
          "label": "Public Transit"
        },
        {
          "value": "bicycle",
          "label": "Bicycle"
        },
        {
          "value": "walking",
          "label": "Walking"
        },
        {
          "value": "ride_share",
          "label": "Ride Share"
        },
        {
          "value": "motorcycle",
          "label": "Motorcycle"
        },
        {
          "value": "scooter",
          "label": "Scooter"
        }
      ],
      "conditional": {
        "dependsOn": "level1_question",
        "operator": "equals",
        "value": false,
        "action": "show"
      }
    }
  ]
}
